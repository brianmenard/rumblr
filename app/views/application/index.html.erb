<div class='row'>
    <div class='col-md-2'></div>
    <div class='col-md-8 center white'>
        <% if !user_signed_in? %>
        <%= link_to "Log in", new_user_session_path, class:"white"%> or <%= link_to "Sign up", new_user_registration_path, class:"white" %>
        <% else %>
        You are now signed in :) 
        <% end %>
    </div>
    <div class='col-md-2'></div>
</div>

<div class="white">
<% @users.each do |user| %>
    username: <%=user.username%><br>
    email: <%=user.email%><br>
<%end%>
</div>