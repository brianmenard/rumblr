<div class='row'>
    <div class='col-md-1'></div>
    <div class='col-md-6'>
        <h1 class="light_gray">Discover</h1>
        <% @posts.each do |post|%>
            <%= render partial: "post", locals: {post: post} %>
        <% end %>
    </div>
    <div class='col-md-2'>
        <%= render partial: "layouts/side_panel", locals: {following_list: @following} %>
    </div>
    <div class='col-md-1'></div>
</div>