<div class='row'>
    <div class='col-md-1'></div>
    <div class='col-md-8'>
        <% @posts.each do |post|%>
            <%= render partial: "post", locals: {post: post} %>
        <% end %>
    </div>
    <div class='col-md-2'>
        <h2>Following</h2>
        <% @following.each do |following| %>
            <%= link_to following.username, following %>
            <br>
        <% end %>
    </div>
    <div class='col-md-1'></div>
</div>