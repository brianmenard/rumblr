<div class='row'>
    <div class='col-md-2'></div>
    <div class='col-md-6'>
        <% @posts.each do |post|%>
            <%= render partial: "post", locals: {post: post} %>
        <% end %>
    </div>
    <div class='col-md-3 hidden-xs hidden-sm'>
        <%= render partial: "layouts/side_panel"%>
    </div>
    <div class='col-md-1'></div>
</div>